{
  "name": "next-drizzle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate:pg --schema ./database/schema.ts --out=./database/migrations",
    "db:migrate": "npx tsx ./database/migrate.ts",
    "db:seed": "npx tsx ./database/seed.ts",
    "db:push": "drizzle-kit push:pg --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@vercel/postgres": "^0.7.2",
    "drizzle-orm": "^0.29.4",
    "jose": "^5.2.2",
    "next": "14.1.1",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8",
    "eslint-config-next": "14.1.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
